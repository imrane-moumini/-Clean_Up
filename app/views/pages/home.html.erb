
 <div class="booking-form">
     <%= form_tag dashboard_path, method: :get do %>
              <%= date_field_tag :day,
              class: "form-group",
              placeholder: "day" %>
               <%= date_field_tag :ville,
              class: "form-group",
              placeholder: "day" %>
               <%= time_field_tag :start_time,
              class: "form-group",
              placeholder: "start_time" %>
               <%= time_field_tag :end_time,
              class: "form-group",
              placeholder: "end_time" %>
              <%= submit_tag "Rechercher", class: "btn btn-flat" %>
            <% end %>
